/* FS Simulator Styles */

#fs-status {
    padding: 1 1 1 1;
    color: $accent;
    height: auto;
}

#fs-breadcrumb {
    padding: 3 3 3 3;
    color: $primary;
    height: auto;
    text-style: bold;
}

#fs-table {
    height: 1fr;
    width: 100%;
    border: solid $primary;
}

/* FS Simulator spacer - aligns with right panel bottom */
#fs-simulator-spacer {
    height: 4;
}
